{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon -w src --exec \"export NODE_ENV=development && npm run lint:js && babel-node src --presets env\"",
    "start:production": "export NODE_ENV=production && node build",
    "build": "babel src -d build",
    "lint:js": "eslint --ext .js src",
    "lint:js-fix": "eslint --fix --ext .js src",
    "test": "jest tests/*",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watchAll"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "apicache": "^0.8.6",
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "googleapis": "^19.0.0",
    "jira-client": "^6.4.0",
    "lodash": "^4.17.13",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "node-cache": "^4.1.1",
    "node-insights": "^1.0.15",
    "path": "^0.12.7",
    "redis": "^2.8.0",
    "rotating-file-stream": "^1.2.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "sinon": "^3.2.1",
    "sinon-stub-promise": "^4.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/config/*"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
