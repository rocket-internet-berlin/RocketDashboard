{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon -w src --exec \"npm run lint:js && babel-node src --presets env\"",
    "start:build": "node build",
    "build": "babel src -d build",
    "lint:js": "eslint --ext .js src/**/*.js",
    "lint:js-fix": "eslint --fix --ext .js src/**/*.js"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "chalk": "^1.1.3",
    "cookie-parser": "~1.4.3",
    "express": "^4.15.2",
    "jade": "~1.11.0",
    "moment": "^2.18.1",
    "morgan": "~1.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
