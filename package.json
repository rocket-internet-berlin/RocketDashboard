{
  "name": "RocketDashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "concurrently": "^3.4.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-interval": "^2.0.1",
    "react-redux": "^5.0.4",
    "recharts": "^0.22.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "sugar": "^2.0.4",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "6.2.10",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.20.0",
    "bootstrap-sass": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^0.28.1",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "json-loader": "0.5.4",
    "lint-staged": "^3.4.1",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.3.1",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "0.5.7",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-validator": "^3.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run client\" \"npm run --prefix server start:dev\"",
    "client": "node scripts/client-start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "yarn test -- --coverage",
    "lint:js": "eslint --ext .js src/**/*.js",
    "lint:js-fix": "eslint --fix --ext .js src/**/*.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --print-width 120 --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/actions/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": "http://localhost:3001"
}
